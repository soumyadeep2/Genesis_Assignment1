def type_of_triangle(a,b,c):

    if a==b and b==c:
        print('Triangle is Equilateral.')
    elif a==b or b==c or a==c:
        print('Triangle is Isosceles.')
    else:
        print('Triangle is Scalane')


#test function

def test_type():
  assert typ_of_triangle(a<0 && b>0)
  answer = false
